  RowBox[{"(*", "Definitions", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"Aidentity", "[", "n_", "]"}], ":=", 
     RowBox[{"IdentityMatrix", "[", 
